---
import Buff from '@/components/Buff.astro'

import { translations } from '../../_translations'

interface Props {
  tag?: string
  hover?: boolean
  class?: string
}

const { tag = '', hover = false, class: className = '' }: Props = Astro.props

const stonepack = 'buff/07/m8s1/214265.webp'
const name = translations.stonepack
const desp = translations.stonepackDesp
---

<Buff icon={stonepack} name={name} nameVariant="yellow" description={desp} tag={tag} hover={hover} class={className} />
