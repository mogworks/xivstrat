  <SeparatorSection id="SBGR" title="SBGR" />
  <SBGR base={base} />
  <AttackSection time="03:17.190" base={base} damage="57000" damageType="physical" />
  <AttackSection time="03:20.219" base={base} damage="57000" damageType="physical" />
  <AttackSection time="03:43.624" base={base} damage="57000" damageType="physical" />
  <AttackSection time="03:46.656" base={base} damage="57000" damageType="physical" />

  <SeparatorSection id="sporesplosion" title="Sporesplosion" />
  <Sporesplosion start="03:47.190" base={base} />

  <AttackSection time="04:19.661" base={base} damage="57000" damageType="physical" />
  <AttackSection time="04:22.696" base={base} damage="57000" damageType="physical" />

  <SeparatorSection id="DDAB" title="Demolition Deathmatch + Abominable Blink" />
  <AttackSection time="04:28.850" base={base} damage="57000" damageType="physical" />
