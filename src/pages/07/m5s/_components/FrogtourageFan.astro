---
import Buff from '@/components/Buff.astro'

import frogtourage_fan from '@/assets/buff/07/m5s/214286.png'
import { translations } from '../_translations'

interface Props {
  tag?: string
  hover?: boolean
  class?: string
}

const { tag = '', hover = false, class: className = '' }: Props = Astro.props
---

<Buff
  icon={frogtourage_fan}
  name={translations.frogtourageFan}
  nameVariant="yellow"
  description={translations.frogtourageFanDesc}
  tag={tag}
  hover={hover}
  class={className}
/>
