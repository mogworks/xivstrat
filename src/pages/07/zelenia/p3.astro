const dutyId = '07/zelenia'

const base = '02:48.767'
---

<DutyStratLayout dutyId={dutyId} base={base}>
  <!-- <EventSection>
    这里写P3阶段起手、走位、机制要点等文字内容。
  </EventSection> -->
  <SeparatorSection id="roseblood-bloom-1" title="魔法阵展开·一式" />
  <RoseBloodBloom start={base} num={1} />
  <AlexandrianThunder2 start="02:54.915" />
  <RoseBloodBloom1 />

  <AlexandrianThunder3 start="03:15.416" />
  <RoseFloor />
  <ThornedCatharsis start="03:22.861" />

  <SeparatorSection id="roseblood-bloom-2" title="魔法阵展开·二式" />
  <RoseBloodBloom start="03:35.299" num={2} />
  <RoseBloodBloom2 />
  <!-- 动态部分暂时先隐藏了 -->
  <!-- <RoseBloodBloom2Dynamic /> -->

  <SpecterOfTheLostCast start="03:55.571" />

  <SeparatorSection id="roseblood-bloom-3" title="魔法阵展开·三式" />

  <RoseBloodBloom start="04:10.005" num={3} />
  <RoseBloodBloom3 />

  <!-- <SeparatorSection id="emblazon" title="Emblazon" />

  <SeparatorSection id="explosion" title="Explosion" />

  <SeparatorSection id="thorned-catharsis" title="Thorned Catharsis" /> -->
  <ThornedCatharsis start="04:42.488" />

  <SeparatorSection id="budofvalor-three" title="生成幻影&震惊&凌空错" />

  <BudofValorCast start="04:55.913"/>

  <EscelonsFallCast start="05:08.165"/>

  <!-- <ShockCast start="05:14.852"/> -->
