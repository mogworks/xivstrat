  <AlexandrianBanish2Cast time="05:15.972"/>
  <EscelonsFallP33 />
  

  <!-- <SeparatorSection id="alexandrian-banish-II" title="Alexandrian Banish II" />

  <SeparatorSection id="escelons-fall " title="Escelons' Fall" />

  <SeparatorSection id="stock-break" title="Stock Break 拍地板" /> -->
  <StockBreakCast start="05:33.532" />

  <SeparatorSection id="roseblood-bloom-4" title="魔法阵展开·四式" />

  <RoseBloodBloom start="05:49.927" num={4} />
  <RoseBloodBloom4 />

  <AlexandrianBanish3 start="06:15.411" />
  <ThornedCatharsis start="06:26.547" />

  <SeparatorSection id="budofvalor-four" title="生成幻影&凌空错" />

  <BudofValorCast start="06:40.047"/>

  <EscelonsFallCast start="06:46.194"/>
  <EscelonsFallP34 />


  <!-- <SeparatorSection id="thorned-catharsis" title="Thorned Catharsis" /> -->

  <SeparatorSection id="roseblood-bloom-5" title="魔法阵展开·五式" />

  <RoseBloodBloom start="07:29.945" num={5} />
  <RoseBloodBloom5 />

  <SeparatorSection id="roseblood-bloom-6" title="魔法阵展开·六式" />
  <RoseBloodBloom start="08:14.821" num={6} />
  <RoseBloodBloom6 />

  <SpecterOfTheLostCast start="07:58.374" />
</DutyStratLayout>
