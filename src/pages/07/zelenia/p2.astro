import SeparatorSection from '@/components/section/SeparatorSection.astro'

import PerfumedQuietus from './_components/PerfumedQuietusCast.astro'
import BlessedBarricade from './_mechanics/BlessedBarricade.astro'

const dutyId = '07/zelenia'

const base = 0
---

